  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "15.12.4",
    "@types/socket.io": "^3.0.2",
    "drizzle-kit": "^0.19.13",
    "nodemon": "^3.0.1",
    "prisma": "^5.1.1",
    "ts-node": "10.2.1",
    "typescript": "4.3.5"
  },
  "scripts": {
    "dev": "NODE_ENV=development && nodemon ./src/app.ts",
    "migrate": "yarn prisma migrate dev",
    "revProxy": "caddy run",
    "start": "node dist/src/app.js",
    "build": "yarn run prism-generate && yarn prisma migrate deploy --schema=./src/db/schema.prisma && tsc",
    "deploy": "git push heroku main",
    "prisma-generate": "prisma generate --schema=./src/db/schema.prisma",
    "run-migrations": "drizzle-kit generate:pg --schema=./src/db/schema.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
