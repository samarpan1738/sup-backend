    "dotenv": "^10.0.0",
    "drizzle-orm": "^0.28.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "postgres": "^3.3.5",
    "socket.io": "^4.2.0",
    "yup": "^0.32.9"
  },
  "engines": {
    "node": "16.14.2"
  }
